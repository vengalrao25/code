
#include <stdio.h>
#define MAX_LIMIT 200

int gmax(int max[])
{
    for (int  i = 0  ;i<100 ;i++)
    {
        max[ i ] = i*2;
    }
    return max ; 
}


int main()
{
    int max[MAX_LIMIT];
    gmax(max);
    for (int i = 0  ;i<100 ;i++)
    {
        printf("%d " , max[i]);
    }
    return 0;
}
